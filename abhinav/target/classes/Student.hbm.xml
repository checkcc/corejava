<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "com.hibenate.entity.Student" table = "STUDENT">
      <id name = "id" type = "int" column = "id">
         <generator class="native"/>
      </id>
      <property name = "studentfirstname" column = "first_name" type = "string"/>
      <property name = "studentlastname" column = "last_name" type = "string"/>
      <property name = "rollno" column = "rollno" type = "int"/>
      <property name = "course" column = "course" type = "string"/>
      
   </class>
</hibernate-mapping>